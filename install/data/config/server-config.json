{
  "server": {
    "http_addr": ":4443",
    "tls_key_file": "../certs/server.key",
    "tls_cert_file": "../certs/server.crt"
  },
  "trust_service": {
    "type": "remote",
    "hostname": "notary-signer.staging.informaticagovcloud.com",
    "port": "7899",
    "tls_ca_file": "../certs/ca.crt",
    "key_algorithm": "ecdsa",
    "tls_client_cert": "../certs/server.crt",
    "tls_client_key": "../certs/server.key"
  },
  "logging": {
    "level": "debug"
  },
  "storage": {
    "backend": "mysql",
    "db_url": "server@tcp(mysql:3306)/notaryserver?parseTime=True"
  }
}